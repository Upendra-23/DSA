class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {
        
        int n = nums2.size();
        vector<int> nge(n, -1);
        stack<int> s;
        for(int i=n-1;i>=0;i--){
            while(!s.empty() && nums2[s.top()] <= nums2[i]){
                s.pop();
            }
            if(!s.empty()) nge[i] = nums2[s.top()];
            s.push(i); 
        }
        
        unordered_map<int,int> mp;
        for(int i=0;i<n;i++) mp[nums2[i]] = i;

        vector<int> ans;
        for(auto it:nums1){
            ans.push_back(nge[mp[it]]);
        }
        return ans;
    }
};
