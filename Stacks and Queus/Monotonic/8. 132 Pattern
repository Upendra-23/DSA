class Solution {
public:
    bool find132pattern(vector<int>& nums) {
        int n = nums.size();
        stack<int> s;
        int secondMax = -1e9;
        for(int i=n-1;i>=0;i--){
            if(nums[i] < secondMax) return true;
            while(!s.empty() && nums[s.top()] <= nums[i]){
                secondMax = max(secondMax, nums[s.top()]);
                s.pop();
            }
            s.push(i);
        }
        return false;
    }
};
