class Solution {
    bool static comp(vector<int> &a, vector<int> &b){
        return a[1] < b[1];
    }
public:
    int findMinArrowShots(vector<vector<int>>& points) {
        sort(points.begin(), points.end(), comp);
        int prev_end = points[0][1];
        int ans = 1;
        for(int i =1; i < points.size(); ++i){
            if(points[i][0] > prev_end){
                ++ans;
                prev_end = points[i][1];
            }
        }
        return ans;
    }
};
