class Solution {
public:
    vector<vector<int>> insert(vector<vector<int>>& intervals, vector<int>& newInterval) {
        map<int,int> mp;
        for(auto &it:intervals){
            mp[it[0]]++;
            mp[it[1]]--;
        }
        mp[newInterval[0]]++;
        mp[newInterval[1]]--;

        vector<vector<int>> ans;
        int start, count = 0;
        for(auto &it:mp){
            if(count == 0) start = it.first;
            count += it.second;
            if(count == 0){
                ans.push_back({start, it.first});
            }
        }
        return ans;
    }
};
