class Solution {
public:
    int maximumPopulation(vector<vector<int>>& logs) {
        map<int,int> mp;
        for(auto it:logs){
            mp[it[0]]++;
            mp[it[1]]--;
        }

        int ans = 0;
        int maxpopulation = 0, sum = 0;
        for(auto &it:mp){
            sum += it.second;
            if(sum > maxpopulation){
                maxpopulation = sum;
                ans = it.first;
            }
        }
        return ans;
    }
};
