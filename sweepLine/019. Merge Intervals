class Solution {
public:
    vector<vector<int>> merge(vector<vector<int>>& intervals) {
        map<int,int> mp;
        for(auto it:intervals) {
            mp[it[0]]++;
            mp[it[1]]--;
        }

        int count = 0, start;
        vector<vector<int>> ans;
        for(auto &it: mp){
            if(count == 0) start = it.first;
            count += it.second;
            if(count == 0){
                ans.push_back({start, it.first});
            }
        }
        return ans;
    }
};
