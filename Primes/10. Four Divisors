class Solution {
    int getdiv(int n) {
        int cnt = 0, sum = 0;
        for(int i=1;i*i<=n;i++) {
            if(n % i == 0) {
                cnt++;
                sum += i;
                if((n / i) != i) {
                    cnt++;
                    sum += (n / i);
                }
            }
        }
        if(cnt == 4) return sum;
        return 0;
    }
public:
    int sumFourDivisors(vector<int>& nums) {
        int cnt = 0;
        for(auto n: nums) {
            cnt += getdiv(n);
        }
        return cnt;
    }
};
