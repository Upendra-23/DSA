class Solution {
    bool isPrime(int n) {
        if(n <= 1) return false;
        int cnt = 0;
        for(int i=2;i*i<=n;i++) {
            if(n % i == 0) {
                return false;
            }
        }
        return true;
    }
public:
    bool completePrime(int num) {
        string s = to_string(num);
        for(int i=0;i<s.size();i++) {
            string pf = s.substr(0, i+1);
            int pval = stoi(pf);
            if(isPrime(pval) == false)
                return false;
        }

        for(int i=s.size()-1;i>=0;i--) {
            string sf = s.substr(i);
            int sval = stoi(sf);
            if(isPrime(sval) == false)
                return false;
        }

        return true;
    }
};
