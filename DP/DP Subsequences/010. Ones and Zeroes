class Solution {
    unordered_map<int,pair<int,int>> mp;
    int fun(int ind, int m, int n, vector<string> &strs, vector<vector<vector<int>>> &dp) {
        if(ind < 0) return 0;

        if(dp[ind][m][n] != -1) return dp[ind][m][n];
        int notpick = 0+fun(ind-1, m, n, strs, dp);
        pair<int,int> nm = mp[ind];
        int pick = 0;
        if(nm.first <= n && nm.second <= m)
            pick = 1+fun(ind-1, m - nm.second, n - nm.first, strs, dp);

        return dp[ind][m][n] = max(pick, notpick);
    }
public:
    int findMaxForm(vector<string>& strs, int m, int n) {
        int size = strs.size();
        for(int i=0;i<size;i++) {
            int n = 0, m = 0;
            for(auto it:strs[i]) {
                if(it == '1') n++;
                else m++;
            }
            mp[i] = {n, m};
        }
        vector<vector<vector<int>>> dp(size, vector<vector<int>> (m+1, vector<int> (n+1, -1)));
        return fun(size-1, m, n, strs, dp);
    }
};
